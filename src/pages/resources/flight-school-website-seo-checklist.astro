};

const checklistName = "Flight School Website & SEO Checklist";

const seoData = {
  pageTitle:
    "Free Flight School Website & SEO Checklist | Right Rudder Marketing",
  pageDescription:
    "Download the FREE Website & SEO Performance Checklist and discover exactly what's keeping qualified leads from finding your flight school. Get actionable strategies to improve your website, rank higher on Google, and convert more visitors into enrolled students.",
